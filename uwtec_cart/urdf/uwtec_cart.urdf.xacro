<?xml version="1.0"?>
<robot name="uwtec_cart" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:arg name="namespace" default="" />

  <material name="orange">
    <color rgba="0.8 0.4 0.0 1.0"/>
  </material>

  <material name="black">
    <color rgba="0 0 0 1.0"/>
  </material>

  <material name="white">
    <color rgba="1 1 1 1.0"/>
  </material>

  <!-- Base Footprint -->
  <link name="base_footprint"/>
  <joint name="base_footprint_to_base_link" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="base_footprint"/>
    <child link="base_link"/>
  </joint>

  <!-- Base Link -->
  <link name="base_link">
    <visual>
      <origin xyz="0.151427 -0 0.5" rpy="0 0 0"/>
      <geometry>
        <box size="2.01142 1 0.568726"/>
      </geometry>
      <material name="orange"/>
    </visual>

    <collision>
      <origin xyz="0.151427 -0 0.5" rpy="0 0 0"/>
      <geometry>
        <box size="2.01142 1 0.568726"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0.151427 -0 0.5" rpy="0 0 0"/>
      <mass value="1.14395"/>
      <inertia
        ixx="0.126164"
        ixy="0.0"
        ixz="0.0"
        iyy="0.416519"
        iyz="0.0"
        izz="0.481014"/>
    </inertial>
  </link>

  <!-- left wheel Link -->
  <joint name="left_wheel_joint" type="continuous">
    <origin xyz="-0.554283 0.625029 0.3" rpy="-1.5707 0 0"/>
    <parent link="base_link"/>
    <child link="left_wheel"/>
    <axis xyz="0 0 1"/>
    <dynamics damping="0.2"/>
  </joint>

  <link name="left_wheel">
    <visual>
      <geometry>
        <cylinder radius="0.3" length="0.25"/>
      </geometry>
      <material name="black"/>
    </visual>

    <collision>
      <geometry>
        <cylinder radius="0.3" length="0.25"/>
      </geometry>
    </collision>

    <inertial>
      <mass value="2"/>
      <inertia ixx="0.145833"
               ixy="0.0"
               ixz="0.0"
               iyy="0.145833"
               iyz="0.0"
               izz="0.125"/>
    </inertial>
  </link>

  <!-- right wheel Link -->
  <joint name="right_wheel_joint" type="continuous">
    <origin xyz="-0.554283 -0.625029 0.3" rpy="-1.5707 0 0"/>
    <parent link="base_link"/>
    <child link="right_wheel"/>
    <axis xyz="0 0 1"/>
    <dynamics damping="0.2"/>
  </joint>

  <link name="right_wheel">
    <visual>
      <geometry>
        <cylinder radius="0.3" length="0.25"/>
      </geometry>
      <material name="black"/>
    </visual>

    <collision>
      <geometry>
        <cylinder radius="0.3" length="0.25"/>
      </geometry>
    </collision>

    <inertial>
      <mass value="2"/>
      <inertia ixx="0.145833"
               ixy="0.0"
               ixz="0.0"
               iyy="0.145833"
               iyz="0.0"
               izz="0.125"/>
      </inertial>
  </link>

  <!-- caster frontal wheel Link -->
  <joint name="caster_joint" type="fixed">
    <origin xyz="0.957138 -0 0.2" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="caster"/>
  </joint>

  <link name="caster">
    <visual>
      <geometry>
        <sphere radius="0.2"/>
      </geometry>
      <material name="white"/>
    </visual>

    <collision>
      <geometry>
        <sphere radius="0.2"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0.957138 -0 0.2" rpy="0 0 0"/>
      <mass value="0.005"/>
      <inertia
        ixx="0.1"
        ixy="0.0"
        ixz="0.0"
        iyy="0.1"
        iyz="0.0"
        izz="0.1"/>
    </inertial>
  </link>

  <joint name="imu_joint" type="fixed">
    <parent link="base_link"/>
    <child link="imu_link"/>
    <origin xyz="0.0 0 0.068" rpy="0 0 0"/>
  </joint>

  <link name="imu_link"/>

  <!-- Add the gps link so we get a static transform for the GPS -->
  <joint name="gps_joint" type="fixed">
    <parent link="base_link"/>
    <child link="gps_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

  <link name="gps_link"/>
</robot>
